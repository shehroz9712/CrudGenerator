{
  "name": "shehroz/crud-generator",
  "description": "Laravel package to generate full CRUD (controllers, models, requests, repositories, policies, migrations, seeders, and Tailwind views) with repository pattern, role-based policies, and optional API routes.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "crud",
    "generator",
    "repository-pattern",
    "policies",
    "tailwind",
    "api",
    "admin"
  ],
  "authors": [
    {
      "name": "Muhammad Shehroz",
      "email": "shehroz9712@gmail.com",
      "homepage": "https://www.linkedin.com/in/muhammadshehroz97/"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3|^8.4",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Shehroz\\CrudGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shehroz\\CrudGenerator\\CrudGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}