{
  "name": "shehroz/crud-generator",
  "description": "Laravel CRUD Generator with roles, permissions, policies, and API support",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Shehroz",
      "email": "shehroz9712@gmail.com",
      "homepage": "https://www.linkedin.com/in/muhammadshehroz97/"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../CrudGenerator"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shehroz\\CrudGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shehroz\\CrudGenerator\\CrudGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}